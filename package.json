    "webpack-cli": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^2.5.0",
    "xterm": "^4.9.0",
    "xterm-addon-fit": "^0.4.0",
    "yorkie": "^2.0.0"
  },
  "appName": "OceanBase Developer Center",
  "build": {
    "extends": "build/electron-build.config.js",
